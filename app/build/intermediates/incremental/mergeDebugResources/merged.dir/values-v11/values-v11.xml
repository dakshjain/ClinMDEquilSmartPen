    <declare-styleable name="HorizontalCalendarView">
        <!-- Text color for all dates except selected Date.-->
        <attr format="color" name="textColorNormal"/>
        <!-- Text color for selected Date. -->
        <attr format="color" name="textColorSelected"/>
        <!-- Background color for selector bar. -->
        <attr format="color" name="selectorColor"/>
        <!-- Background color for selected date item. -->
        <attr format="color" name="selectedDateBackground"/>
        <!-- Text size for month name. -->
        <attr format="dimension" name="textSizeMonthName"/>
        <!-- Text size for day number. -->
        <attr format="dimension" name="textSizeDayNumber"/>
        <!-- Text size for day name. -->
        <attr format="dimension" name="textSizeDayName"/>
    </declare-styleable>
    <declare-styleable name="ZoomImageView">
        <attr format="boolean" name="zoomage_zoomable"/>
        <attr format="boolean" name="zoomage_translatable"/>
        <attr format="boolean" name="zoomage_restrictBounds"/>
        <attr format="boolean" name="zoomage_animateOnReset"/>
        <attr format="boolean" name="zoomage_doubleTapToZoom"/>
        <attr format="float" name="zoomage_doubleTapToZoomScaleFactor"/>
        <attr format="boolean" name="zoomage_autoCenter"/>
        <attr format="float" name="zoomage_minScale"/>
        <attr format="float" name="zoomage_maxScale"/>
        <attr format="enum" name="zoomage_autoResetMode">
            <enum name="UNDER" value="0"/>
            <enum name="OVER" value="1"/>
            <enum name="ALWAYS" value="2"/>
            <enum name="NEVER" value="3"/>
        </attr>
    </declare-styleable>
